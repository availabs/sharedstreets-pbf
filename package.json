{
  "name": "sharedstreets-pbf",
  "version": "0.3.0",
  "description": "SharedStreets, a 'digital commons' for the street",
  "main": "index",
  "types": "index.d.ts",
  "browser": "dist/sharedstreets-pbf.min.js",
  "files": [
    "index.js",
    "index.ts",
    "index.d.ts",
    "proto",
    "dist"
  ],
  "scripts": {
    "pretest": "tsc",
    "test": "node test.js",
    "posttest": "tslint index.ts --project tsconfig.json",
    "docs": "documentation readme --shallow index.js --section=API",
    "bench": "node bench.js",
    "prebuild": "npm run protobuf && npm run protobuf-ts",
    "build": "tsc && npm run browserify && npm run uglify",
    "protobuf": "pbjs -t static-module -w commonjs -o proto/sharedstreets.js proto/sharedstreets.proto",
    "protobuf-ts": "pbts -o proto/sharedstreets.d.ts proto/sharedstreets.js",
    "browserify": "browserify index.js -s sharedstreets -o ./dist/sharedstreets.js",
    "uglify": "uglifyjs dist/sharedstreets.js -o dist/sharedstreets.min.js -cm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sharedstreets/sharedstreets-js.git"
  },
  "keywords": [
    "sharedstreets",
    "streets",
    "shared",
    "pbf",
    "proto"
  ],
  "author": "SharedStreets",
  "contributors": [
    "Denis Carriere <@DenisCarriere>",
    "Kevin Webb <@kpwebb>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sharedstreets/sharedstreets-js/issues"
  },
  "homepage": "https://github.com/sharedstreets/sharedstreets-js#readme",
  "devDependencies": {
    "@types/node": "*",
    "benchmark": "^2.1.4",
    "browserify": "*",
    "documentation": "*",
    "tape": "*",
    "tslint": "*",
    "typescript": "*",
    "uglifyjs": "*"
  },
  "dependencies": {
    "@turf/helpers": "6.0.0-beta.3",
    "protobufjs": "^6.8.4",
    "sharedstreets-types": "*"
  }
}
